      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="age != null" >
        #{age,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  
  <update id="updateUSER" parameterType="com.star.model.User">
  	update user set name=#{name} where age=#{age}
  </update>
</mapper>